{
  "name": "odoo-debugging-mgm-tool",
  "displayName": "Odoo Debugging Management Tool",
  "description": "A Visual Studio Code extension for managing Odoo debugging.",
  "version": "0.2.0",
  "publisher": "odoo-ps",
  "author": {
    "name": "odoo-ps"
  },
  "license": "MIT",
  "homepage": "https://github.com/odoo-ps/vscode-extension-odoo-debugging-mgm#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/odoo-ps/vscode-extension-odoo-debugging-mgm.git"
  },
  "bugs": {
    "url": "https://github.com/odoo-ps/vscode-extension-odoo-debugging-mgm/issues"
  },
  "keywords": [
    "odoo",
    "debugging",
    "python",
    "postgresql",
    "development",
    "project-management"
  ],
  "engines": {
    "vscode": "^1.100.0"
  },
  "categories": [
    "Other",
    "Debuggers",
    "Extension Packs"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "odoo-debugging-mgm-tool-OdooTools",
          "icon": "resources/odoo.svg",
          "title": "Odoo Tools"
        }
      ]
    },
    "views": {
      "odoo-debugging-mgm-tool-OdooTools": [
        {
          "id": "projectSelector",
          "name": "Projects"
        },
        {
          "id": "repoSelector",
          "name": "Repos"
        },
        {
          "id": "dbSelector",
          "name": "Databases"
        },
        {
          "id": "moduleSelector",
          "name": "Modules"
        },
        {
          "id": "testingSelector",
          "name": "Testing"
        },
        {
          "id": "workspaceSettings",
          "name": "Settings"
        }
      ]
    },
    "commands": [
      {
        "command": "projectSelector.create",
        "title": "Create Project",
        "icon": "resources/icons/create.svg"
      },
      {
        "command": "projectSelector.setup",
        "title": "Setup Odoo",
        "icon": "resources/icons/setup.svg"
      },
      {
        "command": "projectSelector.delete",
        "title": "Delete Project",
        "icon": "resources/icons/delete.svg"
      },
      {
        "command": "projectSelector.editSettings",
        "title": "Project Settings",
        "icon": "$(gear)"
      },
      {
        "command": "projectSelector.duplicateProject",
        "title": "Duplicate Project",
        "icon": "resources/icons/create.svg"
      },
      {
        "command": "projectSelector.exportProject",
        "title": "Export Project",
        "icon": "resources/icons/download.svg"
      },
      {
        "command": "projectSelector.importProject",
        "title": "Import Project",
        "icon": "resources/icons/restore.svg"
      },
      {
        "command": "projectSelector.selectProject",
        "title": "Select Project"
      },
      {
        "command": "odoo-debugger.quickProjectSearch",
        "title": "Quick Project Search",
        "category": "Odoo Debugger"
      },
      {
        "command": "projectSelector.refresh",
        "title": "Refresh",
        "icon": "resources/icons/refresh.svg"
      },
      {
        "command": "repoSelector.selectRepo",
        "title": "Select Repo"
      },
      {
        "command": "repoSelector.refresh",
        "title": "Refresh",
        "icon": "resources/icons/refresh.svg"
      },
      {
        "command": "dbSelector.selectDb",
        "title": "Select Database"
      },
      {
        "command": "dbSelector.create",
        "title": "Create Database",
        "icon": "resources/icons/create_database.svg"
      },
      {
        "command": "dbSelector.restore",
        "title": "Restore Database",
        "icon": "resources/icons/restore.svg"
      },
      {
        "command": "dbSelector.delete",
        "title": "Delete Database",
        "icon": "resources/icons/delete.svg"
      },
      {
        "command": "dbSelector.getDb",
        "title": "Get Database",
        "icon": "resources/icons/download.svg"
      },
      {
        "command": "dbSelector.refresh",
        "title": "Refresh Databases",
        "icon": "resources/icons/refresh.svg"
      },
      {
        "command": "moduleSelector.refresh",
        "title": "Refresh",
        "icon": "resources/icons/refresh.svg"
      },
      {
        "command": "moduleSelector.create",
        "title": "Create Module",
        "icon": "resources/icons/create.svg"
      },
      {
        "command": "moduleSelector.select",
        "title": "Toggle Module"
      },
      {
        "command": "moduleSelector.togglePsaeInternalModule",
        "title": "Toggle psae-internal Module"
      },
      {
        "command": "moduleSelector.installAll",
        "title": "Install All Modules",
        "icon": "$(checklist)"
      },
      {
        "command": "moduleSelector.updateInstalled",
        "title": "Update Installed Modules",
        "icon": "$(arrow-circle-up)"
      },
      {
        "command": "moduleSelector.updateAll",
        "title": "Update All Modules",
        "icon": "$(list-unordered)"
      },
      {
        "command": "moduleSelector.clearAll",
        "title": "Clear All Selections",
        "icon": "$(clear-all)"
      },
      {
        "command": "moduleSelector.viewInstalled",
        "title": "View Installed Modules",
        "icon": "$(database)"
      },
      {
        "command": "testingSelector.refresh",
        "title": "Refresh Testing",
        "icon": "$(refresh)"
      },
      {
        "command": "testingSelector.toggleTesting",
        "title": "Toggle Testing",
        "icon": "$(testing)"
      },
      {
        "command": "testingSelector.addTestTag",
        "title": "Add Test Target",
        "icon": "$(add)"
      },
      {
        "command": "testingSelector.setTestFile",
        "title": "Set Test File",
        "icon": "$(file)"
      },
      {
        "command": "testingSelector.toggleStopAfterInit",
        "title": "Toggle Stop After Init",
        "icon": "$(stop)"
      },
      {
        "command": "testingSelector.removeTestTag",
        "title": "Remove Test Target",
        "icon": "$(remove)"
      },
      {
        "command": "testingSelector.cycleTestTagState",
        "title": "Cycle Test Target State",
        "icon": "$(sync)"
      },
      {
        "command": "workspaceSettings.startServer",
        "title": "Start Server",
        "icon": "resources/icons/debug.svg"
      },
      {
        "command": "workspaceSettings.startShell",
        "title": "Start Shell",
        "icon": "resources/icons/shell.svg"
      },
      {
        "command": "workspaceSettings.refresh",
        "title": "Refresh Settings",
        "icon": "resources/icons/refresh.svg"
      },
      {
        "command": "workspaceSettings.editSetting",
        "title": "Edit Setting"
      }
    ],
    "menus": {
      "view/title": [
        {
          "command": "projectSelector.create",
          "when": "view == projectSelector",
          "group": "navigation@1"
        },
        {
          "command": "projectSelector.setup",
          "when": "view == projectSelector",
          "group": "navigation@2"
        },
        {
          "command": "projectSelector.importProject",
          "when": "view == projectSelector",
          "group": "navigation@3"
        },
        {
          "command": "projectSelector.editSettings",
          "when": "view == projectSelector",
          "group": "navigation@4"
        },
        {
          "command": "workspaceSettings.startServer",
          "when": "view == projectSelector",
          "group": "navigation@5"
        },
        {
          "command": "workspaceSettings.startShell",
          "when": "view == projectSelector",
          "group": "navigation@6"
        },
        {
          "command": "projectSelector.refresh",
          "when": "view == projectSelector",
          "group": "navigation@7"
        },
        {
          "command": "dbSelector.refresh",
          "when": "view == dbSelector",
          "group": "navigation"
        },
        {
          "command": "dbSelector.create",
          "when": "view == dbSelector",
          "group": "navigation"
        },
        {
          "command": "repoSelector.refresh",
          "when": "view == repoSelector",
          "group": "navigation"
        },
        {
          "command": "moduleSelector.installAll",
          "when": "view == moduleSelector && !odoo-debugger.testing_enabled",
          "group": "navigation@1"
        },
        {
          "command": "moduleSelector.updateInstalled",
          "when": "view == moduleSelector && !odoo-debugger.testing_enabled",
          "group": "navigation@2"
        },
        {
          "command": "moduleSelector.updateAll",
          "when": "view == moduleSelector && !odoo-debugger.testing_enabled",
          "group": "navigation@3"
        },
        {
          "command": "moduleSelector.clearAll",
          "when": "view == moduleSelector && !odoo-debugger.testing_enabled",
          "group": "navigation@4"
        },
        {
          "command": "moduleSelector.viewInstalled",
          "when": "view == moduleSelector",
          "group": "navigation@5"
        },
        {
          "command": "moduleSelector.refresh",
          "when": "view == moduleSelector",
          "group": "navigation@6"
        },
        {
          "command": "testingSelector.addTestTag",
          "when": "view == testingSelector",
          "group": "navigation@1"
        },
        {
          "command": "testingSelector.setTestFile",
          "when": "view == testingSelector",
          "group": "navigation@2"
        },
        {
          "command": "testingSelector.refresh",
          "when": "view == testingSelector",
          "group": "navigation@3"
        }
      ],
      "view/item/context": [
        {
          "command": "projectSelector.delete",
          "when": "view == projectSelector",
          "group": "inline"
        },
        {
          "command": "projectSelector.editSettings",
          "when": "view == projectSelector",
          "group": "project"
        },
        {
          "command": "projectSelector.duplicateProject",
          "when": "view == projectSelector",
          "group": "project"
        },
        {
          "command": "projectSelector.exportProject",
          "when": "view == projectSelector",
          "group": "project"
        },
        {
          "when": "view == dbSelector",
          "command": "dbSelector.restore",
          "group": "inline"
        },
        {
          "when": "view == dbSelector",
          "command": "dbSelector.delete",
          "group": "inline"
        },
        {
          "command": "testingSelector.removeTestTag",
          "when": "view == testingSelector && viewItem == testTag",
          "group": "inline"
        }
      ]
    },
    "viewsWelcome": [
      {
        "view": "projectSelector",
        "when": "odoo-debugger.is_active === 'false'",
        "contents": "Open a folder or a workspace for the extension to work.\n[Open Folder](command:workbench.action.files.openFolder)\n[Open Workspace](command:workbench.action.openWorkspace)"
      }
    ],
    "keybindings": [
      {
        "command": "odoo-debugger.quickProjectSearch",
        "key": "ctrl+alt+p",
        "mac": "cmd+alt+p",
        "when": "editorTextFocus || !inputFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "watch:dev": "NODE_ENV=development webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test",
    "build:vsix": "vsce package",
    "version:patch": "npm version patch",
    "version:minor": "npm version minor",
    "version:major": "npm version major"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.100.0",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.25.1",
    "ts-loader": "^9.5.2",
    "typescript": "^5.8.3",
    "webpack": "^5.101.0",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "install": "^0.13.0",
    "jsonc-parser": "^3.3.1",
    "npm": "^11.5.2"
  }
}
